<nav>
    {% set colours = ["blue", "red", "green", "pink", "orange"] %}
    {% for page in collections.navList %}
        {% set colour = colours[loop.index0] %}
        {% if page == collections.navList | last %}
            <a href="{{ page.url }}" class="C({{ colour }})">{{ page.data.title }}</a>
        {% else %}
            <a href="{{ page.url }}" class="C({{ colour }})">{{ page.data.title }}</a> |
        {% endif %}
    {% endfor %}
</nav>
